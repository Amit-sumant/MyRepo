package javaBasic;

import java.util.Scanner;

public class NormalPracticeCode
{
	static int i;
	static int j;
	
	public static void main(String[] args)
	{
				
		//print 1 to 10 in triangle shape
		
		int i=0,j=0,n=4,k=1;
        for(i=1; i<n+1; i++)
        {
            for(j=0; j<i; j++)
                System.out.print(" "+k++);
            System.out.println(" ");
        }
			
	}

}
