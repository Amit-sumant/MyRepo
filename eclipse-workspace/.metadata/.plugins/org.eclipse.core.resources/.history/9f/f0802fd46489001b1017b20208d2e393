package javaInterviewCoding;

import java.util.Scanner;

public class PrintInteger {

	public static void main(String[] args) 
	{
		// enter a string given by user
		
		Scanner sc=new Scanner(System.in);
		
		System.out.println("Enter your Integer value");
		
		int i = sc.nextInt();
		
		System.out.println("Your Integer value is -> " + i);
		
		
		System.out.println("---------------Floating Number-------------------------");
		
		Scanner s = new Scanner(System.in);
		
		System.out.println("Enter Your Float Value");
		
		Float fl=s.nextFloat();

	}
	

}
