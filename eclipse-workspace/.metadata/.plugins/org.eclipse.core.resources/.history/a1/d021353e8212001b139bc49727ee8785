package javaBasic;

import java.util.ArrayList;
import java.util.Scanner;

public class ArrayListMethod {

	public static void main(String[] args) {
		// Array list method
		
		ArrayList<Object> ar=new ArrayList<>();
		ar.add("Hello");
		ar.add("This");
		ar.add("is");
		ar.add("Arraylist");
		
		ArrayList<Object> ar2=new ArrayList<>();
		ar2.add(1);
		ar2.add(2.2);
		ar2.add(5);
		
		/*ar.addAll(ar2);
		
		System.out.println(ar);
		
		System.out.println("*****************************");
		//Print value from any index
		ar.addAll(2, ar2);
		System.out.println(ar);*/
		
		//Contains
		//System.out.println(ar.contains("Hello"));
		
		Scanner sc=new Scanner(System.in);
		Object obj=sc.next("[0-9]*[a-z]*");
		
		System.out.println(ar.addAll(ar2));
		System.out.println(ar);
		System.out.println(ar.contains(obj));
		

	}

}
