package oopsConcept;

public class FunctionsJava {

	public static void main(String[] args) 
	{
		System.out.println("Hello");
		FunctionsJava obj=new FunctionsJava();
		
		System.out.println("ABC Method output is :" + obj.abc());

	}
	
	public int abc()
	{
			
		int a=700;
		int b=800;
		int c=a+b;
		
		return c;
	}

}
