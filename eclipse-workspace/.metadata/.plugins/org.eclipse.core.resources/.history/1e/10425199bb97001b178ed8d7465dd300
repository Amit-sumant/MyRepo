package javaBasic;

public class PrintAlphabet {

	public static void main(String[] args) {
		// Printing alphabet pattern like below
		/*
		 A
		 AB
		 ABC
		 ABCD
		 */
		// 65 means value of A 
		int alpha=65;
		for(int i=0;i<=5;i++)
		{
			for(int j=0; j<=i; j++)
			{
				System.out.print((char)(alpha + j) + " ");
			}
			System.out.println();
		}
		

	}

}
