package javaBasic;

public class JavaLoops {

	public static void main(String[] args) {

		int i=1;
		
		while (i<10)
		{
			System.out.println(i);
			//i=i+1;
			i++;
		}
		
		System.out.println("***********next************");
		//for loop
		
		for (i=1;i<=10;i++)
		{
			System.out.println("Value of this loop is:" + i);
		}
	}

}
